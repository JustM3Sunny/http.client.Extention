{
  "name": "http-client-extension",
  "displayName": "HTTP Client",
  "description": "A powerful HTTP client for VS Code, similar to Postman",
  "version": "0.0.1",
  "publisher": "shannii",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustM3Sunny/http.client.Extention.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:http-client.view",
    "onCommand:http-client.openView"
  ],
  "main": "./dist/extension.js",
  "icon": "resources/icon.png",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "http-client",
          "title": "HTTP Client",
          "icon": "resources/icon.png"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "http-client.view",
        "contents": "Welcome to HTTP Client!\n[Send HTTP Requests](command:http-client.openView)"
      }
    ],
    "views": {
      "http-client": [
        {
          "id": "http-client.view",
          "name": "HTTP Client",
          "type": "webview",
          "contextualTitle": "HTTP Client"
        }
      ]
    },
    "commands": [
      {
        "command": "http-client.openView",
        "title": "Open HTTP Client",
        "category": "HTTP Client"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.18.63",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.0.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "vscode-test": "^1.6.1",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
